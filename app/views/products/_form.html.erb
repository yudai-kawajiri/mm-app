<%# 商品フォーム %>
<%= form_with model: product,
              html: { multipart: true, id: 'product_form' },
              data: { controller: 'form-submit', turbo: false } do |f| %>
  <%= render 'shared/forms/resource_two_column_form',
    f: f,
    basic_fields: 'products/fields',
    detail_fields: 'products/materials_fields',
    resource_name: 'products',
    custom_right_title: t('forms.titles.product_material') %>

<% end %>
