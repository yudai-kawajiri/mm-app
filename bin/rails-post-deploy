#!/usr/bin/env bash
set -e

echo "=========================================="
echo "マイグレーションを実行中..."
echo "=========================================="
bundle exec rails db:migrate

echo "=========================================="
echo "シード処理を開始します..."
echo "=========================================="
bundle exec rails db:seed

echo "=========================================="
echo "デプロイ後処理完了"
echo "=========================================="
